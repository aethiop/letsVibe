{
    "manifest_version": 2,
    "name": "letsVibe",
    "description": "",
    "version": "0.1",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
      "default_popup": "src/index.html",
      "default_title": "letsVibe"
    },
    "permissions": [
      "activeTab",
      "activeTab",
      "unlimitedStorage",
      "declarativeContent",
      "tabs"
    ],
    "offline_enabled": true,
    "background": {
      "scripts": [
        "src/js/background.js",
        "node_modules/gun/gun.js",
        "node_modules/gun/sea.js",
        "node_modules/gun/lib/radix.js",
        "node_modules/gun/lib/radisk.js",
        "node_modules/gun/lib/store.js",
        "node_modules/gun/lib/rindexed.js",
        "node_modules/gun/examples/jquery.js",
      ],
      "persistent": true
    },
    "content_scripts": [
     {
       "matches": ["<all_urls>", "*://*/*"],
       "all_frames": true,
       "run_at": "document_start",
       "js": ["node_modules/gun/examples/jquery.js", "src/js/vibe.js"]
     }
    ]
  }